{
    "extends": ["airbnb-base"],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/no-extraneous-dependencies": ["error", {
            "devDependencies": [
                "**/*.config.js",
                "vite.config.js"
            ]
        }]
    }
}