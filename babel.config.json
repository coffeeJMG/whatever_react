{
    "plugins": [
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragma": "createElement", // JSX 변환 시 사용할 함수 이름
                "pragmaFrag": "Fragment" // Frgament (<></>) 사용시 변환할 이름
            }
        ]
    ],
    // 지원할 브라우저 명시적 정의
    "targets": {
        "chrome": "64",
        "edge": "79",
        "firefox": "67",
        "safari": "12"
    }
}
